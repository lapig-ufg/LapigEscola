{
  "name": "vuero",
  "version": "2.8.1",
  "private": true,
  "type": "module",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": {
    "name": "cssninjaStudio <hello@cssninja.io> (https://cssninja.io)"
  },
  "engines": {
    "node": ">=16.15.0",
    "npm": ">=8"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e -V "
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint"
    ],
    "*.md": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "scripts": {
    "prepare": "jiti ./scripts/prepare-shiki.ts",
    "dev": "run-p dev:vite dev:json-server",
    "dev:ssr": "run-p ssr:dev dev:json-server",
    "dev:vite": "vite --open",
    "dev:json-server": "json-server --read-only --routes ./json-server/routes.json --port 8080 --watch ./json-server/db.json",
    "build": "run-s build:*",
    "build:update-bulma-colors": "bulma-css-vars",
    "build:vite": "cross-env NODE_ENV=production vite build",
    "ssr:build": "run-s build:update-bulma-colors ssr:build:*",
    "ssr:build:client": "cross-env NODE_ENV=production vite build --ssrManifest --outDir dist/client",
    "ssr:build:server": "cross-env NODE_ENV=production vite build --ssr src/entry-server.ts --outDir dist/server",
    "ssr:dev": "jiti server.ts",
    "ssr:serve": "cross-env NODE_ENV=production jiti server.ts",
    "ssr:preview": "run-p ssr:serve preview:json-server",
    "ssg:build": "pnpm build:update-bulma-colors && cross-env NODE_ENV=production jiti build-ssg.ts",
    "ssg:serve": "serve dist -p 3000",
    "preview": "run-p preview:vite preview:json-server",
    "preview:vite": "serve dist -s -p 5000",
    "preview:json-server": "json-server -q --read-only --routes ./json-server/routes.json --host 0.0.0.0 --port 8080 ./json-server/db.json",
    "lint": "run-s lint:doc:fix lint:prettier:fix lint:eslint:fix lint:stylelint:fix ",
    "lint:prettier": "prettier --check \"./src/**/*.(ts|vue|css|scss)\"",
    "lint:prettier:fix": "prettier --write \"./src/**/*.(ts|vue|css|scss)\"",
    "lint:eslint": "eslint --ext .ts,.vue ./src",
    "lint:eslint:fix": "eslint --fix --ext .ts,.vue ./src",
    "lint:stylelint": "stylelint ./src/**/*.{vue,css,scss}",
    "lint:stylelint:fix": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "lint:doc:fix": "run-s lint:doc:fix:*",
    "lint:doc:fix:prettier": "prettier --write \"./documentation/**/*.(md)\"",
    "lint:doc:fix:eslint": "eslint --fix --ext .md ./documentation",
    "test": "run-p test:lint test:tsc test:e2e",
    "test:tsc": "vue-tsc --noEmit",
    "test:lint": "run-p lint:eslint lint:stylelint",
    "test:e2e": "cypress run",
    "test:unit": "echo \"[warning] no unit test implemented yet\"",
    "cypress": "cypress"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "39.0.1",
    "@ckeditor/ckeditor5-vue": "5.1.0",
    "@cssninja/bulma-css-vars": "0.9.2",
    "@iconify/iconify": "3.1.1",
    "@mapbox/mapbox-gl-geocoder": "5.0.1",
    "@minify-html/node": "^0.11.1",
    "@popperjs/core": "2.11.8",
    "@unhead/ssr": "1.3.7",
    "@unhead/vue": "1.3.7",
    "@vee-validate/zod": "4.11.3",
    "@vueform/multiselect": "2.6.2",
    "@vueform/slider": "2.1.7",
    "@vueuse/core": "10.4.0",
    "@vueuse/router": "10.4.0",
    "apexcharts": "3.42.0",
    "billboard.js": "3.9.3",
    "bulma": "0.9.4",
    "dayjs": "1.11.9",
    "defu": "6.1.2",
    "dragula": "3.7.3",
    "dropzone": "6.0.0-beta.2",
