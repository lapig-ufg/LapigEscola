      }
    },
    "overrides": {
      "sass": "npm:sass-embedded@1.66.1",
      "bulma": "npm:@cssninja/bulma@0.9.4"
    },
    "patchedDependencies": {
      "@purge-icons/core@0.9.1": "patches/@purge-icons__core@0.9.1.patch"
